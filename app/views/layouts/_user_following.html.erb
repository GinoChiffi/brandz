<h3>Currently following <%= current_user.follows.count%> brands</h3></br>

<div id="articles" class="transitions-enabled">
    <% current_user.all_following.each do |f|%>
    <div class="box panel panel-default">
       <%= link_to image_tag(f.logo.url(:medium)), brand_path(f.name)%></br>
    </div>
  <% end %>
  </div>
</div>

